{
  "name": "Newbie",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Don't believe everything you read online.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "permissions": ["tabs", "<all_urls>", "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"]
}